# outputs.tf

output "debug_processed_container_definitions" {
  description = "Debug output for processed container_definitions sent to ECS module/resource."
  value = {
    for service_name, service_config in local.ecs_service_configs :
    service_name => service_config.container_definitions # Ou local.services_without_service_connect[service_name].container_definitions
  }
}
